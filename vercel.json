{
  "functions": {
    "api/index.py": {
      "maxDuration": 30,
      "includeFiles": "src/**",
      "excludeFiles": "**/site-packages/{torch,tensorflow,transformers,pandas,numpy,gradio}/**,**/__pycache__/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}